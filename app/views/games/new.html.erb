<%= form_with(model: @game,local: true) do |f| %>
  <div class="form-group">
    <%= f.label :title, 'ゲームタイトル' %>
    <%= f.text_area :title, class: 'form-control'%>
  </div>
  <%= f.submit '作成', class: 'btn btn-primary btn-block' %>
<% end %>
<div>
  <%= link_to '戻る', root_url, class: 'btn btn-secondary' %>
</div>