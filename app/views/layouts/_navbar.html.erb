<%= link_to 'ホーム', root_url %>
<% if logged_in? %>
  <%= link_to current_user.name, user_path(current_user) %>
  <%= link_to 'ユーザー一覧', users_path %>
  <%= link_to 'ログアウト', logout_path, method: :delete %>
<% else %>
  <%= link_to 'ユーザー登録', signup_path %>
  <%= link_to 'ログイン', login_path %>
<% end %>
